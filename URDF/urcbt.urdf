<?xml version="1.0"?>
<robot name="URCBOT">
  <material name="Blue">
    <color rgba="0.0 0.0 0.8 1.0"/>
  </material>

  <material name="Grey2">
    <color rgba="0.9 0.9 0.9 1.0"/>
  </material>

  <material name="Black">
    <color rgba="0.1 0.1 0.1 1.0"/>
  </material>

  <material name="LightGrey">
    <color rgba="0.6 0.6 0.6 1.0"/>
  </material>

  <link name="base_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/><!--need to check if it is the mass of COM or distri..-->
      <mass value="5"/>
      <inertia ixx="100"  ixy="0"  ixz="0" iyy="100" iyz="0" izz="100" />
    </inertial>

    <visual>
      <geometry>
        <box size="6.5 3.3 0.4"/>
      </geometry>
      <material name="Blue" ></material>
    </visual>
  </link>

  <link name="front_right_leg">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/><!--need to check if it is the mass of COM or distri..-->
      <mass value="1"/>
      <inertia ixx="100"  ixy="0"  ixz="0" iyy="100" iyz="0" izz="100" />
    </inertial>

    <visual>
      <geometry>
        <cylinder length="0.50" radius="0.6"/>
      </geometry>
      <origin rpy="1.57075 0 0" xyz="0.0 0.0 0.0"/>
      <material name="Grey2" ></material>
    </visual>
  </link>

  <link name="middle_right_leg">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/><!--need to check if it is the mass of COM or distri..-->
      <mass value="1"/>
      <inertia ixx="100"  ixy="0"  ixz="0" iyy="100" iyz="0" izz="100" />
    </inertial>

    <visual>
      <geometry>
        <cylinder length="0.50" radius="0.6"/>
      </geometry>
      <origin rpy="1.57075 0 0" xyz="0.0 0.0 0.0"/>
      <material name="Grey2" ></material>
    </visual>
  </link>

  <link name="back_right_leg">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/><!--need to check if it is the mass of COM or distri..-->
      <mass value="1"/>
      <inertia ixx="100"  ixy="0"  ixz="0" iyy="100" iyz="0" izz="100" />
    </inertial>

    <visual>
      <geometry>
        <cylinder length="0.50" radius="0.6"/>
      </geometry>
      <origin rpy="1.57075 0 0" xyz="0.0 0.0 0.0"/>
      <material name="Grey2" ></material>
    </visual>
  </link>

  <link name="front_left_leg">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/><!--need to check if it is the mass of COM or distri..-->
      <mass value="1"/>
      <inertia ixx="100"  ixy="0"  ixz="0" iyy="100" iyz="0" izz="100" />
    </inertial>

    <visual>
      <geometry>
        <cylinder length="0.50" radius="0.6"/>
      </geometry>
      <origin rpy="1.57075 0 0" xyz="0.0 0.0 0.0"/>
      <material name="Grey2" ></material>
    </visual>
  </link>

  <link name="middle_left_leg">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/><!--need to check if it is the mass of COM or distri..-->
      <mass value="1"/>
      <inertia ixx="100"  ixy="0"  ixz="0" iyy="100" iyz="0" izz="100" />
    </inertial>

    <visual>
      <geometry>
        <cylinder length="0.50" radius="0.6"/>
      </geometry>
      <origin rpy="1.57075 0 0" xyz="0.0 0.0 0.0"/>
      <material name="Grey2" ></material>
    </visual>
  </link>

  <link name="back_left_leg">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/><!--need to check if it is the mass of COM or distri..-->
      <mass value="1"/>
      <inertia ixx="100"  ixy="0"  ixz="0" iyy="100" iyz="0" izz="100" />
    </inertial>

    <visual>
      <geometry>
        <cylinder length="0.50" radius="0.6"/>
      </geometry>
      <origin rpy="1.57075 0 0" xyz="0.0 0.0 0.0"/>
      <material name="Grey2" ></material>
    </visual>
  </link>

  <joint name="base_to_front_right_leg" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="front_right_leg"/>
    <origin xyz="2.5 2.0 0.0"/>
    <calibration rising="0.0"/>
    <dynamics damping="0.0" friction="0.0"/>
    <limit effort="30" velocity="1.0" lower="-2.2" upper="0.7" />
    <safety_controller k_velocity="10" k_position="15" soft_lower_limit="-2.0" soft_upper_limit="0.5" />
  </joint>

  <joint name="base_to_back_right_leg" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="back_right_leg"/>
    <origin xyz="-2.5 2.0 0.0  "/>
    <calibration rising="0.0"/>
    <dynamics damping="0.0" friction="0.0"/>
    <limit effort="30" velocity="1.0" lower="-2.2" upper="0.7" />
    <safety_controller k_velocity="10" k_position="15" soft_lower_limit="-2.0" soft_upper_limit="0.5" />
  </joint>

  <joint name="base_to_middle_right_leg" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="middle_right_leg"/>
    <origin xyz="0 2.0 0.0 "/>
    <calibration rising="0.0"/>
    <dynamics damping="0.0" friction="0.0"/>
    <limit effort="30" velocity="1.0" lower="-2.2" upper="0.7" />
    <safety_controller k_velocity="10" k_position="15" soft_lower_limit="-2.0" soft_upper_limit="0.5" />
  </joint>

  <joint name="base_to_front_left_leg" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="front_left_leg"/>
    <origin xyz="2.5 -2.0 0.0"/>
    <calibration rising="0.0"/>
    <dynamics damping="0.0" friction="0.0"/>
    <limit effort="30" velocity="1.0" lower="-2.2" upper="0.7" />
    <safety_controller k_velocity="10" k_position="15" soft_lower_limit="-2.0" soft_upper_limit="0.5" />
  </joint>

  <joint name="base_to_back_left_leg" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="back_left_leg"/>
    <origin xyz="-2.5 -2.0 0.0  "/>
    <calibration rising="0.0"/>
    <dynamics damping="0.0" friction="0.0"/>
    <limit effort="30" velocity="1.0" lower="-2.2" upper="0.7" />
    <safety_controller k_velocity="10" k_position="15" soft_lower_limit="-2.0" soft_upper_limit="0.5" />
  </joint>

  <joint name="base_to_middle_left_leg" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="middle_left_leg"/>
    <origin xyz="0 -2.0 0.0  "/>
  </joint>

</robot>
